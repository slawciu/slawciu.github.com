---
layout: nil
---
<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>    
    <title>Sławek Rudawski - dev blog</title>
    <description>{{ site.description | xml_escape }}</description>
    <link>{{ site.url }}</link>
    <atom:link href="https://slawciu.github.io/rss.dajsiepoznac.xml" rel="self" type="application/rss+xml" />
    {% for post in site.categories["dajsiepoznac"] %}

        <item>
          <title>{{ post.title | xml_escape }}</title>
          <description>{{ post.content | xml_escape }}</description>
          <pubDate>{{ post.date | date:"%a, %d %b %Y %T %z" }}</pubDate>
          <link>{{ site.url }}{{ post.url }}</link>
          <guid>{{ site.url }}{{ post.url }}</guid>
        </item>

    {% endfor %}
  </channel>
</rss>